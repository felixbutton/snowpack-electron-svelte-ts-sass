{
  "name": "snowpack-electron-svelte-ts-sass",
  "version": "0.0.1",
  "description": "Template for snowpack, electron and svelte with Typescript and Sass",
  "license": "MIT",
  "author": "Felix Knopf",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixbutton/snowpack-electron-svelte-ts-sass.git"
  },
  "keywords": [
    "snowpack",
    "electron",
    "svelte"
  ],
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run electron\"",
    "start": "set NODE_ENV=dev && snowpack dev --open none",
    "build": "snowpack build",
    "electron": "electron ."
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "svelte": "^3.24.0"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-sass": "^1.2.0",
    "@snowpack/plugin-svelte": "^3.4.0",
    "@snowpack/plugin-typescript": "^1.1.1",
    "@snowpack/web-test-runner-plugin": "^0.1.5",
    "@testing-library/svelte": "^3.0.0",
    "@types/snowpack-env": "^2.3.2",
    "@web/test-runner": "^0.10.0",
    "autoprefixer": "^10.2.1",
    "electron": "^11.2.1",
    "node-sass": "^5.0.0",
    "snowpack": "^2.18.4",
    "svelte-preprocess": "^4.0.8",
    "typescript": "^4.0.0"
  }
}